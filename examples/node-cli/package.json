{
    "name": "@chili-publish/studio-sdk-node-cli-example",
    "version": "1.0.0",
    "description": "Example Node.js CLI application using the Studio SDK",
    "private": true,
    "main": "src/index.js",
    "bin": "dist/index.js",
    "pkg": {
        "targets": ["node18-linux-x64"],
        "outputPath": "dist/bin"
    },
    "scripts": {
        "start": "node src/index.js",
        "build": "tsc",
        "dev": "ts-node src/index.ts start -w http://localhost:3001",
        "package": "pkg ."
    },
    "dependencies": {
        "@chili-publish/studio-sdk": "file:../../packages/sdk",
        "commander": "^11.0.0",
        "inquirer": "^8.2.6"
    },
    "devDependencies": {
        "@types/inquirer": "^8.2.6",
        "@types/node": "^18.0.0",
        "pkg": "^5.8.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3"
    }
}