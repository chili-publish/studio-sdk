{
    "name": "@chili-publish/studio-sdk-node-cli-example",
    "version": "1.0.0",
    "description": "Example Node.js CLI application using the Studio SDK",
    "private": true,
    "main": "src/index.js",
    "bin": "dist/index.js",
    "pkg": {
        "targets": [
            "node18-linux-x64"
        ],
        "outputPath": "dist/bin"
    },
    "scripts": {
        "build": "tsc",
        "dev": "ts-node src/index.ts start -w http://localhost:3001",
        "package": "pkg .",
        "demo": "ts-node src/demo-server.ts"
    },
    "dependencies": {
        "@chili-publish/studio-sdk": "file:../../packages/sdk",
        "@types/ws": "^8.18.0",
        "chalk": "^4.1.2",
        "commander": "^11.0.0",
        "ws": "^8.18.1"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "pkg": "^5.8.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3"
    }
}
